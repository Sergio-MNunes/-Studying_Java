public class Ex0103Rectangle {

	private float length;
	private float width;
	
	public Ex0103Rectangle() {
		
	}

	public Ex0103Rectangle(float length, float width) {
		this.length = length;
		this.width = width;
	}
	
	public float getLength() {
		return length;
	}
	
	public void setLength(float length) {
		this.length = length;
	}
	
}